[alias]
	p = pull
	s = status
    graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
	b = branch
	ba = branch -a
	d = diff --color
	co = checkout
	cm = commit -m
	a = add .
	ame = commit --amend
	po = push origin
	sm = submodule
	fp = fetch --prune
	c = commit
	pu = pull
	dlb = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
	cv = commit -v
	con = config --global
